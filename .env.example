SESSION_PARTITIONED_COOKIE=false

SANCTUM_STATEFUL_DOMAINS=localhost,localhost:5173,localhost:8000,127.0.0.1,127.0.0.1:5173,127.0.0.1:8000,[::1],[::1]:5173,[::1]:8000
